{
  "schemaVersion": 1,
  "id": "telefabric",
  "version": "${version}",
  "name": "TeleFabric",
  "description": "A bridge to connect your fabric server with Telegram bots",
  "authors": [
    "JasonZYT"
  ],
  "contact": {
    "sources": "https://github.com/Jasonzyt/telefabric"
  },
  "license": "MIT",
  "icon": "assets/telefabric/icon.png",
  "environment": "server",
  "entrypoints": {
    "main": [
      "com.jasonzyt.telefabric.Telefabric"
    ]
  },
  "mixins": [
    "telefabric.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=${loader_version}",
    "fabric-api": "*",
    "minecraft": "${minecraft_version}"
  }
}